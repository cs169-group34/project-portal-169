#container

  =render partial: "main/sidebar"

  =render partial: "main/settings_login"

  #content.projects
    %h1 List of Projects
  
    =link_to("Add Project", new_project_path)
  
    %ul
      - @projects.each do |project|
        %li
          =image_tag(asset_path("project_no_image.png"))
          %h2= project.title
          %p= project.content
        - if project != @projects.last
          <hr/>
        