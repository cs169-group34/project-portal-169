= "Name: #{@student_team.name}"
%br
= "Email: #{@student_team.email}" 
%br
= "GSI: #{@gsi.name}"
%br
= "GSI Email: #{@gsi.email}" 
%br
= "Project: #{@project.title}"
%br
= "Project Content:" 
%br
= "#{@project.content}"


%h2 Iterations
- @iterations.each do |iteration|
  .iteration
    %h3=iteration.iteration_string
    %p
    %strong Date: 
    ="#{iteration.created_at_string}"
    %p
    %strong User Stories:
    %br
    =iteration.user_stories
    %p
    %strong Comments:
    %br
    =iteration.comments
    
- if @is_instructor
  = form_tag "/student_teams/#{@student_team.id}/assign" do
    = label :assign, :project, 'Project to assign:'
    %br
    = text_field :assign, 'project', id: 'assign to project'
    %p
    = label :assign, :instructor, 'Project to assign:'
    %br
    = text_field :assign, 'instructor', id: 'assign to instructor'
    %p
    = submit_tag 'Assign', id: 'Assign'
